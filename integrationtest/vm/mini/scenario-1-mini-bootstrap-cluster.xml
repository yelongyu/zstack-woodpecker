<?xml version="1.0" encoding="UTF-8"?>
<config>
<basicConfig>
  <zstackManagementIp>$zstackManagementIp</zstackManagementIp>
  <zstackAdminPassword>$zstackAdminPassword</zstackAdminPassword>
  <zstackPkg>$zstackPkg</zstackPkg>
  <zstackISO>$zstackISO</zstackISO>
</basicConfig>

<deployerConfig xmlns="http://zstack.org/schema/zstack">
  <hosts>
    <host uuid="$hostUuid">
      <vms>
        <vm name="$vmName1" vmInstranceOfferingUuid="$vmInstranceOfferingUuid2" 
          imageUuid="$imageUuid" imageUsername="$imageUsername" imagePassword="$imagePassword" vmSerial="$vm1SN">
          <l3Networks>
            <l3Network uuid="$vmL3Uuid">
            </l3Network>
            <l3Network uuid="$vmL3Uuid2">
            </l3Network>
            <l3Network uuid="$vmL3Uuid3">
            </l3Network>
            <l3Network uuid="$vmL3Uuid4">
            </l3Network>
            <l3Network uuid="$vmL3Uuid5">
            </l3Network>
          </l3Networks>
          <nodeRef>$node1Name</nodeRef>
	  <hostRef>$hostName</hostRef>
          <mHaHostRef>$hostName</mHaHostRef>
        </vm>
        <vm name="$vmName2" vmInstranceOfferingUuid="$vmInstranceOfferingUuid2" 
          imageUuid="$imageUuid" imageUsername="$imageUsername" imagePassword="$imagePassword" vmSerial="$vm2SN">
          <l3Networks>
            <l3Network uuid="$vmL3Uuid">
            </l3Network>
            <l3Network uuid="$vmL3Uuid2">
            </l3Network>
            <l3Network uuid="$vmL3Uuid3">
            </l3Network>
            <l3Network uuid="$vmL3Uuid4">
            </l3Network>
            <l3Network uuid="$vmL3Uuid5">
            </l3Network>
          </l3Networks>
          <nodeRef>$node2Name</nodeRef>
	  <hostRef>$hostName2</hostRef>
          <mHaHostRef>$hostName2</mHaHostRef>
        </vm>
      </vms>
    </host>
  </hosts>
</deployerConfig>
</config>
